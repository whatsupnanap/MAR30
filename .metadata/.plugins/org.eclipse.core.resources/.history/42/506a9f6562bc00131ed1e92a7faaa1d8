var openWindow = function(){
	
	var message = "can you see this?";
	
	var newWindow = Ti.UI.createWindow({
		title: message,
		backgroundColor: "blue"
	});
	
	newWindow.open();
	
};


button.addEventListener("click",openWindow);

//var message = "can you see this?";
exports.message;
//alert(message);


win.add(button);