var mainWindow = Ti.UI.createWindow({
	name:"Main Window",
	id: 1,
	backgroundColor:"#FFF",
	backgroundImage:"deimenssion.png",
	backgroundRepeat:true

});

var header = Ti.UI.createView({
	backgroundColor:"#333",
	borderRadius:5,
	borderWidth:1,
	//width: 200,
	height: 50,
	top:20,
	left:10,
	right:10,
	bottom:10
});

var appName = Ti.UI.creatLabel({
	text: "Dice Game",
	color:"#FFF",
	font:{fontSize: "16dp",fontFamily:"Helvetica"}
});

header.add(appName);
mainWindow.add(header);
//open the windowâ†“
mainWindow.open();

