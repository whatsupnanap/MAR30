var mainWindow = Ti.UI.createWindow({
	backgroundColor : "#fff"
});

var topName = Ti.UI.createLabel({
	text: "minions",
	color:"#71efd3",
	font:{fontSize: "20dp", fontFamily:"Helvetica"}
	
});

var buttonTop = Ti.UI.createView({
	backgroundColor:"#8375bc",
	borderRadius: 5,
	borderWidth:1,
	
});

var bottomName = Ti.UI.createLabel({
	text: "others",
	color:"#71efd3",
	font:{fontSize: "20dp", fontFamily:"Helvetica"}
	
});

var buttonBtm = Ti.UI.createView({
	backgroundColor:"#8375bc",
	borderRadius: 5,
	borderWidth:1,
	
});


mainWindow.add(buttonTop,buttonBtm);

buttonTop.add(topName);
buttonBtm.add(bottomName);

mainWindow.open();
